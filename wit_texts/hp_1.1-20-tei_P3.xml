<lg xml:id="HP11"> <l> oṃ ādināthāya namo 'stu tasmai yenopadiṣṭā haṭhayogavidyayā/</l> <l> virājate pronnatarājasaudham āroḍhum icchor adhirohiṇīva//</l> </lg> <lg xml:id="HP12"> <l> praṇamya śrī guruṃ nāthaṃ svātmārāmeṇa dhīmatā/</l> <l> kevalaṃ rājayogāya haṭhavidyopadiśyate// </l> </lg> <lg xml:id="HP13"> <l> bhrāṃtā bahumatadhvānte rājayogam ajānataḥ/</l> <l> haṭhapradīpikāṃ cakre svātmārāmaḥ kṛpākaraḥ// </l></lg> <lg xml:id="HP14"> <l> haṭhavidyāṃ hi matsyendragorakṣādyā vijānate/</l> <l> ātmārāmo 'thavā yogī jānīte tatprasādataḥ// </l> </lg> <lg xml:id="HP15"> <l> śrīādināthamatsyendraṃśāradānandabhairavāḥ/</l> <l> cauraṅgīmīnagorakṣavirūpākṣabileśayā// </l> </lg> <lg xml:id="HP16"> <l> manthānabhairavo yogī siddhabudhasya kasyapaḥ/</l> <l> pauraṇṭakaḥ surānandaḥ siddhapādaś ca tarpaṭī// </l> </lg> <lg xml:id="HP17"> <l> karaṇī pūjyapādaś ca bilvanātho nirañjanaḥ/</l> <l> kapālībindunāthaś ca kākacaṇḍīśvarādayaḥ// </l> </lg> <lg xml:id="HP18"> <l> sukṣamaḥ prabhudevaś ca ghoḍācolī gha ṭiṇṭiṇiḥ/</l> <l> vālukir nāgabodhaś ca caṇḍikāpālikas tathā// </l> </lg> <lg xml:id="HP19"> <l> ityādayo mahā....{}{}</l>/ <l> {} {}{} {} {}// </l> </lg> <lg xml:id="HP110"> <l> {}{}{} {}{}/</l> <l> {}{}{}{}{}// </l> </lg> <lg xml:id="HP111"> <l> {} {} {} {} {}{}/</l> <l> {}{} {} {} {} {}// </l> </lg> <lg xml:id="HP112"> <l> {}{}{} {}/</l> <l> </l> <l>   </l> <l>  </l> <l> </l> <l>  </l> <l> {}{}{}{} {}{}//</l> <l> </l> </lg> <lg xml:id="HP113"> <l> {}{}{}{}{}{}/</l> <l> {}{}{}{} {}{}/</l> <l> {} {}{} {}{}/</l> <l> {} {}{}{} {}{}{}// </l> </lg> <lg xml:id="HP114"> <l> {}{}{} {}{}{}/</l> <l> {}{}{} {}{}// </l> </lg> <lg xml:id="HP115"> <l> {}{} {}{}/</l> <l> {} {} ca {}{}//</l> </lg> <lg xml:id="HP116"> <l> {}{}{}{}/</l> <l> {} {}{}// </l> </lg> <lg xml:id="HP116a"> <l> </l> <l>    </l> <l>   </l> </lg> <lg xml:id="HP116b"> <l> </l> <l>  </l> <l> </l> </lg> <lg xml:id="HP116c"> <l> </l> </lg> <lg xml:id="HP117"> <l> {}{}{}/</l> <l> {}{}{}{}{}//</l> </lg> <lg xml:id="HP118"> <l> 